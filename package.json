{
  "name": "three-shooter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "build:test": "dotenv -e .env.test -- next build",
    "prebuild:test": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true});\"",
    "build:prod": "dotenv -e .env.production -- next build",
    "start": "next start -p 3001",
    "start:test": "dotenv -e .env.test -- next start -p 3001",
    "start:prod": "dotenv -e .env.production -- next start -p 3001",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@types/react": "19.1.14",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.178.1",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "jsdom": "^25.0.1",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/jest-dom": "^6.6.3"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "lil-gui": "^0.20.0",
    "next": "15.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "three": "^0.178.0",
    "zustand": "^5.0.8"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --cache --fix"
    ]
  }
}
